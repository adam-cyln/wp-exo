{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wp-exo/rw-post",
	"version": "0.1.0",
	"title": "WP exo post",
	"category": "design",
	"icon": "excerpt-view",
	"description": "Un bloc Article qui affiche le résumé d’une publication, avec l’image, l’auteur et la catégorie. ",
	"supports": {
		"html": false
	},
	"attributes": {
		"postID": { 
			"type": "integer"
		},
		"showCategory": { 
			"type": "boolean",
			"default": true
		},
		"showAuthor": { 
			"type": "boolean",
			"default": true
		},
		"showImage": { 
			"type": "boolean",
			"default": true
		}
	},
	"textdomain": "wp-exo",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}